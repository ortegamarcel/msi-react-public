{
  "name": "serien-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enumify": "^1.0.4",
    "flux": "^3.1.3",
    "material-ui": "^0.20.0",
    "material-ui-rating": "^2.0.0",
    "mdi-react": "^3.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "npm run deploy-windows && npm run deploy-in-docker",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy-windows": "copy-item -Recurse build ../webserver",
    "deploy-linux": "cp -R build webserver",
    "deploy-in-docker": "docker rm -f serializer || true && docker build -t msi-react/webserver ../webserver && docker run -p 80:80 -d --name serializer msi-react/webserver",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
